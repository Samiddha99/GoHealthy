{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",
    "name": "Go Healthy - Health is Your Right",
    "short_name": "Go Healthy",
    "description": "A web base healthcare service",
    "shortcuts": [
        {
            "name": "Find a bed in nearest hospotal",
            "short_name": "Find bed",
